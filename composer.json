{
    "name": "godsgood33/csv-reader",
    "description": "This library simplifies reading CSV files",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpunit/php-code-coverage": "^9.2",
        "phpstan/phpstan": "^1.2"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ryan Prather",
            "email": "godsgood33@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Godsgood33\\CSVReader\\": "src",
            "Godsgood33\\CSVReaderTests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse src --level=9",
        "coverage": "vendor/bin/phpunit . --coverage-html=coverage"
    }
}